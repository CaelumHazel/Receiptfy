    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "expo": {
      "plugins": [
        "@react-native-firebase/app"
      ]
    },
    "extra": {
      "eas": {
        "projectId": "222c3c82-6556-4180-8eb7-f13c2ec3eff7"
